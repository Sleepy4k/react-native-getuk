{
  "expo": {
    "name": "Getukan",
    "slug": "getukan",
    "version": "1.0.0",
    "description": "Melihat toko getuk disekitar anda",
    "orientation": "portrait",
    "icon": "./assets/logogetuk.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logogetuk.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.benjamin4k.getukan",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.benjamin4k.getukan",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logogetuk.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.MANAGE_DOCUMENTS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/logogetuk.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your client."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiYmVuamFtaW40ayIsImEiOiJjbG93eGZibW0xYTgyMmtxMDV0c3R2cDcwIn0.gJucuQzpbjNDHfUIdbFUuQ"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7b1518c4-a747-4ce7-93f3-38fd294d4ec8"
      }
    }
  }
}

{
  "expo": {
    "name": "react-native-getuk",
    "slug": "react-native-getuk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logogetuk.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logogetuk.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "assets": [
      "./assets/logogetuk.png"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logogetuk.png",
        "backgroundColor": "#ffffff",
        "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
      }
    },
    "web": {
      "favicon": "./assets/logogetuk.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}
